{
  "ignorePatterns": [
    "!**/*"
  ],
  "overrides": [
    {
      "files": [
        "*.js",
        "*.jsx"
      ],
      "extends": [
        "plugin:@nx/javascript"
      ],
      "rules": {}
    },
    {
      "files": [
        "*.ts",
        "*.tsx"
      ],
      "extends": [
        "plugin:@nx/typescript"
      ],
      "rules": {}
    },
    {
      "files": [
        "src/**/*.{ts,js,tsx,jsx}"
      ],
      "extends": [
        "plugin:prettier/recommended"
      ],
      "rules": {}
    }
  ],
  "extends": [
    "plugin:playwright/recommended"
  ],
  "plugins": [
    "@nx"
  ]
}
